<?xml version="1.0"?>
<Container version="2">
  <Name>MindsDB</Name>
  <Repository>mindsdb/mindsdb</Repository>
  <Registry>https://hub.docker.com/r/mindsdb/mindsdb/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/mrjamiebowman/unraid-templates/issues</Support>
  <Project/>
  <Overview>MindsDB gives enterprise databases predictive capabilities powered by machine learning.</Overview>
  <Category>Cloud:</Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/mrjamiebowman/unraid-templates/master/mrjamiebowman/MindsDb.xml</TemplateURL>
  <Icon>https://github.com/mrjamiebowman/unraid-templates/raw/master/images/mindsdb.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/mindsdb/storage</HostDir>
      <ContainerDir>/root/mdb_storage</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Config Type="Port" Name="Port1" Target="47334" Default="47334" Mode="tcp" Display="always-hide" Required="true" Description="Ports"/>
  <Config Type="Port" Name="Port2" Target="47335" Default="47335" Mode="tcp" Display="always-hide" Required="true" Description="Ports"/>
</Container>