<?xml version="1.0"?>
<Container version="2">
  <Name>Debezium</Name>
  <Repository>debezium/connect</Repository>
  <Registry>https://hub.docker.com/r/debezium/connect</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/mrjamiebowman/unraid-templates/issues</Support>
  <Project>https://github.com/mrjamiebowman/unraid-templates</Project>
  <Overview>Debezium</Overview>
  <Category>Tools:</Category>
  <WebUI>http://[IP]:[PORT:8083]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/mrjamiebowman/unraid-templates/master/mrjamiebowman/Debezium.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/mrjamiebowman/unraid-templates/master/images/debezium-logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1676695565</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="KAFKA_BROKER_ID" Target="KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">1</Config>
  <Config Name="KAFKA_ZOOKEEPER_CONNECT" Target="KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">zookeeper:2181</Config>
  <Config Name="KAFKA_LISTENER_SECURITY_PROTOCOL_MAP" Target="KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">PLAINTEXT:PLAINTEXT,PLAINTEXT_INTERNAL:PLAINTEXT</Config>
  <Config Name="KAFKA_ADVERTISED_LISTENERS" Target="KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">PLAINTEXT://localhost:9092,PLAINTEXT_INTERNAL://kafka:29092</Config>
  <Config Name="KAFKA_OFFSETS_TOPIC_REPLICATION_FACTOR" Target="KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">1</Config>
  <Config Name="KAFKA_TRANSACTION_STATE_LOG_MIN_ISR" Target="KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">1</Config>
  <Config Name="KAFKA_TRANSACTION_STATE_LOG_REPLICATION_FACTOR" Target="KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">1</Config>
  <Config Name="KAFKA_AUTO_CREATE_TOPICS_ENABLE" Target="KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">false</Config>

  <Config Name="8083" Target="8083" Default="" Mode="tcp" Description="Container Port: 8083" Type="Port" Display="always" Required="true" Mask="false">8083</Config>
  <Config Name="8084" Target="8084" Default="" Mode="tcp" Description="Container Port: 8084" Type="Port" Display="always" Required="true" Mask="false">8084</Config>
</Container>
